{
    "name": "healthcare-chain-fabric",
    "version": "1.0.0",
    "client": {
        "organization": "${ORG}",
        "connection": {
            "timeout": {
                "peer": {
                    "endorser": "300"
                }
            }
        }
    },
    "organizations": {
        "${ORG}": {
            "mspid": "${ORGMSP}MSP",
            "peers": [
                "peer0.${ORG}.healthcare.com"
            ],
            "certificateAuthorities": [
                "ca.${ORG}.healthcare.com"
            ]
        }
    },
    "peers": {
        "peer0.${ORG}.healthcare.com": {
            "url": "grpc://localhost:${P0PORT}",
            "tlsCACerts": {
                "pem": "${PEERPEM}"
            },
            "grpcOptions": {
                "ssl-target-name-override": "peer0.${ORG}.healthcare.com",
                "hostnameOverride": "peer0.${ORG}.healthcare.com"
            }
        }
    },
    "certificateAuthorities": {
        "ca.${ORG}.healthcare.com": {
            "url": "http://localhost:${CAPORT}",
            "caName": "ca.${ORG}.healthcare.com",
            "tlsCACerts": {
                "pem": "${CAPEM}"
            },
            "httpOptions": {
                "verify": false
            }
        }
    }
}
