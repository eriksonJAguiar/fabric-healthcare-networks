{
    "name": "Healthcare-logs",
    "version": "0.0.4",
    "description": "Contract Store Healthcare logs",
    "main": "index.js",
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
