    "license": "Apache-2.0",
    "dependencies": {
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "fabric-contract-api": "1.4.2",
        "fabric-shim": "1.4.2",
        "helmet": "^3.21.1",
        "morgan": "^1.9.1"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "eslint": "^6.3.0",
        "mocha": "^6.2.0",
        "nyc": "^14.1.1",
        "sinon": "^7.4.1",
        "sinon-chai": "^3.3.0",
        "winston": "^3.2.1"
    },
    "nyc": {
        "exclude": [
            ".eslintrc.js",
            "coverage/**",
            "test/**"
